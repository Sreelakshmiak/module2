package debugng;
import java.io.*;
public class Testcode2 {
	public static void main(String[] args) {
	int a;
	int b;
	int x;
	int a1[]= {2,5,7,8,5};
	int b1[]= {23,43,27,32,12};
	int x1[]= {18,19,20,21,22};
	//ax^2+2x^2+2ab
	 for (int i = 0; i < a1.length; i++) {
         a = a1[i];
         b = b1[i];
         x = x1[i];
         double result = a * Math.pow(x, 2) + 2 * Math.pow(x, 2) + 2 * a * b;
         System.out.println("Result for element " + i + ": " + result);
     }

     // TODO Auto-generated method stub
 }






	
	}


